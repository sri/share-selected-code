{
    "name": "share-selection-and-path",
    "displayName": "Share Selection and Path",
    "description": "Share Selection and Path to third party sites",
    "version": "0.0.1",
    "publisher": "sri",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.shareSelectionAndPath.slack",
        "onCommand:extension.shareSelectionAndPath.jira"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.shareSelectionAndPath.slack",
                "title": "Share Selection And Path for Slack"
            },
            {
                "command": "extension.shareSelectionAndPath.jira",
                "title": "Share Selection And Path for JIRA"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "clipboardy": "1.2.3",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "@types/clipboardy": "^1.1.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
